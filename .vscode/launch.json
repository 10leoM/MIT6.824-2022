{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run MR Sequential (Word Count)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrsequential.go",
            "args": [
                "${workspaceFolder}/src/mrapps/wc.so",
                "${workspaceFolder}/src/main/pg-being_ernest.txt"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "Run MR Sequential (Indexer)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrsequential.go",
            "args": [
                "${workspaceFolder}/src/mrapps/indexer.so",
                "${workspaceFolder}/src/main/pg-being_ernest.txt"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "MapReduce Master (All Files)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrmaster.go",
            "args": [
                "${workspaceFolder}/src/main/pg-being_ernest.txt",
                "${workspaceFolder}/src/main/pg-dorian_gray.txt",
                "${workspaceFolder}/src/main/pg-frankenstein.txt",
                "${workspaceFolder}/src/main/pg-grimm.txt",
                "${workspaceFolder}/src/main/pg-huckleberry_finn.txt",
                "${workspaceFolder}/src/main/pg-metamorphosis.txt",
                "${workspaceFolder}/src/main/pg-sherlock_holmes.txt",
                "${workspaceFolder}/src/main/pg-tom_sawyer.txt"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "MapReduce Master (Single File)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrmaster.go",
            "args": [
                "${workspaceFolder}/src/main/pg-being_ernest.txt"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "MapReduce Worker (Word Count)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrworker.go",
            "preLaunchTask": "Build wc.so",
            "args": [
                "${workspaceFolder}/src/mrapps/wc.so"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "MapReduce Worker (Indexer)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrworker.go",
            "preLaunchTask": "Build indexer.so",
            "args": [
                "${workspaceFolder}/src/mrapps/indexer.so"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "Debug Master with Race Detector",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrmaster.go",
            "args": [
                "${workspaceFolder}/src/main/pg-being_ernest.txt"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main",
            "buildFlags": "-race"
        },
        {
            "name": "Debug Worker with Race Detector",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main/mrworker.go",
            "preLaunchTask": "Build wc.so",
            "args": [
                "${workspaceFolder}/src/mrapps/wc.so"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main",
            "buildFlags": "-race"
        },
        {
            "name": "Run Lab1 Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/src/main",
            "args": [
                "-v"
            ],
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "Run Test Script",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}/src/main/test-mr.sh",
            "console": "integratedTerminal",
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${workspaceFolder}/src/main"
        },
        {
            "name": "Debug Current File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "env": {
                "GO111MODULE": "off",
                "GOOS": "linux"
            },
            "cwd": "${fileDirname}"
        }
    ]
}